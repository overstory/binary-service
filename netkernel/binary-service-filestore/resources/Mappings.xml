<config>
	<endpoint>
		<verbs>SOURCE,SINK,DELETE</verbs>
		<grammar>
			<active>
				<identifier>active:osBinary</identifier>
				<argument name="id"/>
				<argument name="mimetype" min="0" max="1"/>
				<argument name="hash" min="0" max="1"/>
			</active>
		</grammar>
		<request>
			<identifier>active:groovy</identifier>
			<argument name="operator">res:/resources/osBinaryFileStore.gy</argument>
			<argument name="id">arg:id</argument>
			<argument name="mimetype">arg:mimetype</argument>
			<argument name="hash">arg:hash</argument>
		</request>
	</endpoint>
</config>
